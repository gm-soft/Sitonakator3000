<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>

  <appSettings>

    <!-- Имя папки, где лежат файлы проекта в рутовой директории сайта -->
    <add key="website_common_dir" value="site" />

    <!-- Имя папки, где лежат все архивы по подпапкам -->
    <add key="website_arch_dir" value="arch" />

    <!-- имена папок, которые нужно игнорировать в процессах удаления/копирования -->
    <add key="specific_content_folder_names" value="_appeals,_uploads,_video" />

  </appSettings>

</configuration>